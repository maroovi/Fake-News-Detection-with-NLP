# Routes
GET     /                           controllers.BasicController.index

GET     /inputTypesForm             controllers.BasicController.inputTypesForm
POST    /inputTypesFormPost         controllers.BasicController.inputTypesFormPost

GET     /analysisPost               controllers.BasicController.analysisPost


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
